<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>


<fx:root type="javafx.scene.control.ListCell" alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="300.0"
         xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <HBox fx:id="item_container">
        <children>
            <Label fx:id="lb_item" prefWidth="170.0" text="item内容..."/>
            <Button fx:id="bt_confirm" defaultButton="true"
                    mnemonicParsing="false" prefWidth="50.0" text="确认">
                <HBox.margin>
                    <Insets right="8.0"/>
                </HBox.margin>
            </Button>
            <Button fx:id="bt_exclude" cancelButton="true" mnemonicParsing="false" prefWidth="50.0" text="排除"/>
        </children>
        <padding>
            <Insets left="10.0" right="10.0"/>
        </padding>
    </HBox>
</fx:root>
